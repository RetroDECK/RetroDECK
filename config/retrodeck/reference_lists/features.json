              "abxy_button": true
            }
          ]
        },
        "mgba_libretro": {
          "name": "mGBA",
          "description": "Game Boy Advance Libretro Core",
          "system": "gba",
          "properties": [
            {
              "borders": true,
              "rewind": true,
              "abxy_button": true
            }
          ]
        }
