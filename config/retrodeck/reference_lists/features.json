    "mame": {
      "description": "Multiple Arcade Machine Emulator",
      "name": "MAME",
      "url": "https://retrodeck.readthedocs.io/en/latest/wiki_emulator_guides/mame/mame-guide/",
      "system": [
        "arcade"
      ],
      "launch": "mame",
      "cli-arg": "-help"
    },
    "citra": {
      "description": "A legacy Nintendo 3DS Emulator (via Ponzu)",
